{
	"name": "Selenized (pastel)",
	"include": "./Selenized.json",
	"tokenColors": [
		{
			"scope": [
				"string.unquoted",
				"string.quoted.multi"
			],
			"settings": {
				"foreground": "#aedf7f"
			}
		},
		{
			"name": "Name of the defined entity",
			"scope": "entity.name",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffe283"
			}
		},
		{
			"scope": "keyword.operator.assignment",
			"settings": {
				"foreground": "#ffe283"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#8ab6ff"
			}
		},
		{
			"name": "reserved language variables like this, super, self",
			"scope": "variable.language",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#8ab6ff"
			}
		},
		{
			"scope": "constant.character.set",
			"settings": {
				"foreground": "#97f0e4"
			}
		},
		{
			"name": "stuff from library/framework",
			"scope": "support.function",
			"settings": {
				"foreground": "#d0b1fd"
			}
		},
		{
			"scope": "storage.type.format",
			"settings": {
				"foreground": "#ffb484"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#ffaadf"
			}
		}
	]
}
